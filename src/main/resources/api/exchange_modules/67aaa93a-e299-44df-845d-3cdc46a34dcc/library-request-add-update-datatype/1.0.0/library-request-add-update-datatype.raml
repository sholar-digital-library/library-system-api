#%RAML 1.0 DataType

description: This datatype is for the request payload for the add and update the library

type: object
properties:
  library_branch_name:
    type: string
    required: true
    example: "vizag"
  library_phone_no:
    type: number
    required: true
    example: 7894561235
  library_email:
    type: string
    required: true
    example: "vizaglibrary@schoolar.com" 
  address:
    type: string
    required: true
    example: "oppsite to HDFC bank"